Cell["\<\
// As before, using Table and Random[Integer], set AliceBasis and BobBasis to \
random lists of 0 and 1 and To model the data positions that Eve interferes \
with, set EvesDrop to a random list of 0 and 1\
\>", "Text",
 CellChangeTimes->{{3.9394363378724174`*^9, 
  3.939436365928793*^9}},ExpressionUUID->"ca5b0451-cb8a-c84c-bedc-\
9b0a4351c7bd"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"AliceBasis", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"RandomInteger", "[", 
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "]"}], ",", 
     RowBox[{"{", "60", "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"BobBasis", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"RandomInteger", "[", 
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "]"}], ",", 
     RowBox[{"{", "60", "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"EvesDrop", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"RandomInteger", "[", 
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], "]"}], ",", 
     RowBox[{"{", "60", "}"}]}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.939421251587158*^9, 3.939421251588663*^9}, 
   3.9394221670335865`*^9, {3.9394280772772217`*^9, 3.939428085231535*^9}},
 CellLabel->"In[79]:=",ExpressionUUID->"b21dfc61-e08f-df4e-bcc8-8b8e2bf4bb47"],

Cell["\<\
// To model the exchange of basis information, set EqualBases to 1 at each \
position where AliceBasis equals BobBasis and 0 otherwise.\
\>", "Text",
 CellChangeTimes->{{3.939436372141245*^9, 
  3.9394363966209965`*^9}},ExpressionUUID->"2af6a12f-6021-c446-bf95-\
da452ded5d93"],

Cell[BoxData[
 RowBox[{
  RowBox[{"EqualBases", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"AliceBasis", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "==", 
        RowBox[{"BobBasis", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], ",", "1", ",", "0"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "60"}], "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.9394280922351856`*^9, 3.939428092236191*^9}},
 CellLabel->"In[82]:=",ExpressionUUID->"97af3fc1-fcc8-4b4a-9e0c-5cbd09c0f836"],

Cell["\<\
// Since we know in advance when Eve will be eves dropping, set ReliableData \
to a table with 1 at each position where EqualBases is 1 and EvesDrop is 0\
\>", "Text",
 CellChangeTimes->{{3.939436414164156*^9, 
  3.9394364194395237`*^9}},ExpressionUUID->"b32da966-cf93-504b-978b-\
d387ec7c265e"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ReliableData", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"EqualBases", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "==", "1"}], "&&", 
        RowBox[{
         RowBox[{"EvesDrop", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "==", "0"}]}], ",", "1", ",", "0"}],
       "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "60"}], "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.9394280988741913`*^9, 3.9394280988751926`*^9}},
 CellLabel->"In[83]:=",ExpressionUUID->"3a41d087-f01b-684a-a5be-67caecb42660"],

Cell["\<\
// basically created AliceData and BoboData that is to be changed in the \
coming for loop, so just made with with zeros for now.\
\>", "Text",
 CellChangeTimes->{{3.9394365347626324`*^9, 3.9394365593854847`*^9}, {
  3.9394365902565193`*^9, 
  3.939436610584202*^9}},ExpressionUUID->"f9cf540b-7710-3d40-8b3d-\
cf8a154e7b08"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"AliceData", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", "60", "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"BobData", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", "60", "}"}]}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.939428110689543*^9, 3.939428110690542*^9}, {
   3.9394364900586834`*^9, 3.9394364927636414`*^9}, 3.9394365513804855`*^9},
 CellLabel->
  "In[111]:=",ExpressionUUID->"e7ef8319-9134-f349-9db0-3e55b7de9a81"],

Cell["\<\
// Now we model the measurement of entangled states. When the entangled data \
(which we do not represent, we only model its measurement) is not interfered \
with, Alice and Bob measure a random bit, but always the same random bit. \
When there is interference, the bits measured by Alice and Bob are \
uncorrelated random bits. Using a For loop, set AliceData and BobData to \
lists of random bits which must be the same at each position where \
ReliableData is 1\
\>", "Text",
 CellChangeTimes->{{3.93943661576215*^9, 
  3.939436631678156*^9}},ExpressionUUID->"f1fb7c6c-d790-9747-b5d1-\
8de0b11c3573"],

Cell[BoxData[
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", 
    RowBox[{"i", "<=", "60"}], ",", 
    RowBox[{"i", "++"}], ",", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"ReliableData", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "==", "1"}], ",", 
       RowBox[{
        RowBox[{"AliceData", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "=", 
        RowBox[{
         RowBox[{"BobData", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "=", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"0", ",", "1"}], "}"}], "]"}]}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"AliceData", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "=", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"0", ",", "1"}], "}"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"BobData", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "=", 
         RowBox[{"RandomInteger", "[", 
          RowBox[{"{", 
           RowBox[{"0", ",", "1"}], "}"}], "]"}]}], ";"}]}], "]"}], ";"}]}], 
   "]"}], ";"}]], "Input",
 CellChangeTimes->{{3.9394281169984856`*^9, 3.9394281169984856`*^9}, {
  3.939436570797331*^9, 3.9394365778167744`*^9}},
