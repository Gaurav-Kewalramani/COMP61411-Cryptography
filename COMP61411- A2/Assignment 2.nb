 RowBox[{"ContourPlot", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"y", "^", "2"}], "==", 
    RowBox[{
     RowBox[{"x", "^", "3"}], "-", 
     RowBox[{"3", " ", "x"}], "+", "3"}]}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"-", "3"}], ",", "3"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"y", ",", 
     RowBox[{"-", "4"}], ",", "4"}], "}"}], ",", 
   RowBox[{"Axes", "->", "True"}], ",", 
   RowBox[{"Epilog", "->", 
    RowBox[{"{", 
     RowBox[{"Line", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "3"}], ",", "4"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"4", ",", 
          RowBox[{"-", "3"}]}], "}"}]}], "}"}], "]"}], "}"}]}]}], 
  "]"}]], "Input",
